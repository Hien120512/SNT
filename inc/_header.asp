<!-- + header -->
<header class="clearfix">
  <div class="headerInner wrapper clearfix">
    
    <!-- + logo -->
    <h1><a href="/" title="RMS">RMS</a></h1>
    <!-- / logo -->

    <!-- + login -->
    <div id="login">
      <a href="/ClientResources/Default.asp" id="btnClientLogin" title="Client Login">Client Login</a>
      <form id="form_login" method="post" action="" class="clearfix">
        <h3>Private Client Area</h3>
        <ul class="benefits">
          <li>
            <h4>High-touch Service &amp; Support</h4>
            <p>Extensive resources on RMS Models and Tools.</p>
          </li>
          <li>
            <h4>The RMS Client Library</h4>
            <p>Comprehensive Catastrophe Updates.</p>
          </li>
          <li>
            <h4>Capital Markets Resources</h4>
            <p>Paradex Indices &amp; ILS Portfolio Management</p>
          </li>
        </ul>
        <fieldset>
          <label id="form_login_username_label" for="form_login_username">User Name</label>
          <input type="text" name="Login" id="form_login_username" value="" />
          
          <a href="#" id="form_login_forgot_password">Forgot?</a>
          <label id="form_login_password_label" for="form_login_password">Password</label>
          <input type="password" name="password" id="form_login_password" value="" />
          
          <input type="checkbox" name="remember" id="form_login_remember" value="1" />
          <label id="form_login_remember_label" for="form_login_remember">Remember me</label>
          
          <input type="hidden" value="Submit" name="Action.x" alt="Submit" />
          <button type="submit" name="btn_login" class="btn btnSm" id="form_login_login" value="">Log In</button>
        </fieldset>
      </form>
    </div>
    <!-- / login -->
  
